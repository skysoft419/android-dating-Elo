<?xml version="1.0" encoding="utf-8"?>
<set
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:anim/overshoot_interpolator"
    android:fillAfter="true">

    <translate
        android:startOffset="20"
        android:fromXDelta="0%p"
        android:toXDelta="5%p"
        android:duration="25" />

    <translate
        android:startOffset="45"
        android:fromXDelta="0%p"
        android:toXDelta="-12%p"
        android:duration="25" />

    <translate
        android:startOffset="70"
        android:fromXDelta="0%p"
        android:toXDelta="12%p"
        android:duration="25" />

    <translate
        android:startOffset="95"
        android:fromXDelta="0%p"
        android:toXDelta="-10%p"
        android:duration="25" />

    <translate
        android:startOffset="120"
        android:fromXDelta="0%p"
        android:toXDelta="5%p"
        android:duration="25" />
</set>